<?xml version="1.0" encoding="utf-8"?>
<project>

	<!--IOS-->
	<section if="ios">
		<config:ios>
			<cocoapod name="GA-SDK-IOS" version="3.0.1" />
		</config:ios>
	</section>

	<!--ANDROID-->
	<section if="android">
		<android extension="com.gameanalytics.MyGameAnalytics"/>
		<dependency name="gameanalytics" path="dependencies/gameanalytics"/>
	</section>

	<!--HTML5-->
	<section if="html5">
		<dependency path="dependencies/ga-js-sdk.js" embed="true" />
	</section>

</project>
